{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "aws/aws-sdk-php": "^3.176.2",
        "blade-ui-kit/blade-heroicons": "^1.0.0",
        "blade-ui-kit/blade-ui-kit": "^0.2.0",
        "calebporzio/sushi": "^v2.1.1",
        "eluceo/ical": "2.x-dev",
        "fideloper/proxy": "^4.4.1",
        "friendsofphp/php-cs-fixer": "v2.18.4",
        "fruitcake/laravel-cors": "^v2.0.3",
        "guzzlehttp/guzzle": "^7.3.0",
        "laravel/framework": "^v8.34.0",
        "laravel/scout": "^v8.6.0",
        "laravel/tinker": "^v2.6.1",
        "league/flysystem-aws-s3-v3": "^1.0.29",
        "livewire/livewire": "v2.4.1",
        "meilisearch/meilisearch-laravel-scout": "v0.12.5",
        "spatie/laravel-activitylog": "^3.17.0",
        "spatie/laravel-mail-preview": "4.0.1",
        "spatie/laravel-permission": "^4.0.1",
        "spatie/laravel-query-builder": "^3.3.4",
        "spatie/laravel-ray": "^1.17.1",
        "spatie/laravel-tail": "^4.3.2",
        "squizlabs/php_codesniffer": "^3.5.8",
        "stevebauman/purify": "v4.0.0"
    },
    "require-dev": {
        "facade/ignition": "^2.6.0",
        "fakerphp/faker": "^1.13.0",
        "laravel/sail": "^v1.4.9",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^v5.3.0",
        "phpunit/phpunit": "^9.5.4",
        "barryvdh/laravel-debugbar": "^v3.5.2",
        "barryvdh/laravel-ide-helper": "v2.9.1",
        "beyondcode/laravel-dump-server": "^1.7.0",
        "jasonmccreary/laravel-test-assertions": "^v1.1.0",
        "laracasts/cypress": "^1.3.0",
        "laravel-shift/blueprint": "v1.22.0",
        "psalm/plugin-laravel": "^v1.4.2",
        "vimeo/psalm": "^4.6.4"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
          "@php artisan ide-helper:generate",
          "@php artisan ide-helper:meta"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
